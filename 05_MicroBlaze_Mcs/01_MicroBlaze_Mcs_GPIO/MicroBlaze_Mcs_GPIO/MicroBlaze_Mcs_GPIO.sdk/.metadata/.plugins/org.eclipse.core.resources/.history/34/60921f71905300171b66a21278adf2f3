 *      Author: John
 */

#include "xparameters.h"
#include "xil_cache.h"
#include "xiomodule.h"

int main()
{
   Xil_ICacheEnable();
   Xil_DCacheEnable();

   {
	   XStatus  status;
	   XIOModule ioModule;
	   int i;
	   status = XIOModule_Initialize(&ioModule, XPAR_IOMODULE_0_DEVICE_ID);

	   status = XIOModule_Start(&ioModule);

	   while(1)
	   {
		   for(i=0; i < 100000; i++)
		   {
			   XIOModule_DiscreteWrite(&ioModule, 1, i);
		   }
	   }
   }

   Xil_DCacheDisable();
   Xil_ICacheDisable();
   return 0;
}
